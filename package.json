{
  "name": "@etalab/adresses-locales",
  "version": "0.0.0",
  "description": "Agrégation des bases Adresse locales",
  "repository": "git@github.com:etalab/adresses-locales.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "contours:download": "curl http://etalab-datasets.geo.data.gouv.fr/contours-administratifs/2019/geojson/communes-100m.geojson.gz | gunzip > communes-100m.json",
    "lint": "xo",
    "build": "node --max_old_space_size=8192 build"
  },
  "dependencies": {
    "@etalab/bal": "^1.5.1",
    "@etalab/decoupage-administratif": "^0.8.0",
    "@livingdata/keyv": "^4.1.0",
    "@livingdata/keyv-sqlite": "^2.1.0",
    "@slack/web-api": "^6.0.0",
    "@turf/turf": "^5.1.6",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "decompress": "^4.2.1",
    "dotenv": "^8.2.0",
    "epsg-index": "^1.1.0",
    "fs-extra": "^9.1.0",
    "gdal-next": "^1.0.3",
    "got": "^11.8.1",
    "hasha": "^5.2.2",
    "iconv-lite": "^0.6.2",
    "js-yaml": "^4.0.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.4",
    "node-xlsx": "^0.16.1",
    "papaparse": "^5.3.0",
    "proj4": "^2.7.0",
    "strip-bom-buf": "^2.0.0",
    "tmp-promise": "^3.0.2",
    "worker-farm": "^1.7.0"
  },
  "devDependencies": {
    "xo": "^0.44.0"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "camelcase": "off",
      "unicorn/prefer-string-slice": "off",
      "prefer-named-capture-group": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-set-has": "off",
      "unicorn/prefer-module": "off",
      "unicorn/import-style": "off",
      "comma-dangle": "off",
      "import/extensions": "off",
      "node/prefer-global/process": "off",
      "node/prefer-global/buffer": "off",
      "unicorn/no-array-for-each": "off",
      "arrow-body-style": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/prefer-object-from-entries": "off",
      "unicorn/numeric-separators-style": "off"
    }
  },
  "engines": {
    "node": ">= 14"
  }
}
